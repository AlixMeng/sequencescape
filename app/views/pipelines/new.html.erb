<% add :title, "Pipelines" -%>
<% add :menu, "All pipelines" => pipelines_url -%>

<h1>CREATE NEW PIPELINE</h1>
<div class="content">
  <div class="info">
  To create a new pipeline, complete the form below. Required fields are marked with <%= required_marker %>.
  </div>
  <div id="sample">
    <% form_for :pipeline, :url => pipelines_path do |form| -%>
      <%= render :partial => "pipeline", :locals => {:read_only => false, :form => form} -%>
    <% end -%>
  </div>
</div>
