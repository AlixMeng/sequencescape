<% max = @failing_request.env.keys.max { |a,b| a.length <=> b.length } -%>
<% @failing_request.env.keys.sort.each do |key| -%>
* <%= "%-*s: %s" % [max.length, key, filter_sensitive_post_data_from_env(key, @failing_request.env[key].to_s.strip)] %>
<% end -%>

* Process: <%= $$ %>
* Server : <%= `hostname -s`.chomp %>
